PImage img;
void setup(){
size(1014,1800);
background(0);
img = loadImage("1.jpg");
int w = img.width;
int h = img.height;
text("Image size: "+w+"x"+h,10,13);
}
void draw(){
  imageMode(CENTER);
  tint(255,127); 
  image(img,img.width/2,img.height/2); 
  tint(255,255); 
 imageMode(CORNERS);
 image(img,img.width/4,img.height/4,3*img.width/4,3*img.height/4);
 
}
