void setup(){
 size(300, 300);
}
void draw(){}
void mouseReleased(){
 background(0);
  fill(0,255,0);
  stroke(255);
   strokeWeight(10);
  ellipse (mouseX, mouseY,80,80);
}
void mousePressed(){
 background(0);
  fill(255,0,0);
  stroke(255,255,0);
  strokeWeight(20);
  ellipse (mouseX, mouseY,80,80);
}
